{
    "$schema": "http://json-schema.rog/draft-04/schema#",
    "type": "object",
    "properties": {
        "/": { "$ref": "https://raw.github.com/zakgrant/json-schema-example/master/entry-schema.json#" }
    },
    "patternProperties": {
        "^(/[^/]+)+$": { "$ref": "https://raw.github.com/zakgrant/json-schema-example/master/entry-schema.json#" }
    },
    "additionalProperties": false,
    "required": [ "/" ]
}
