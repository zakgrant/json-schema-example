{
    "$schema": "http://json-schema.rog/draft-04/schema#",
    "type": "object",
    "properties": {
        "/": { "$ref": "http://github.com/zakgrant/json-schema-example/entry-schema#" }
    },
    "patternProperties": {
        "^(/[^/]+)+$": { "$ref": "http://github.com/zakgrant/json-schema-example/entry-schema#" }
    },
    "additionalProperties": false,
    "required": [ "/" ]
}
